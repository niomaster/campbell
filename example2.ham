class Box
    int value

    fun void init(int value)
        this.value = value

    fun int get()
        return this.value

    fun int add(Box other)
        return this.value + other.get()

fun int answer()
    return 42

fun int applyTwice((int -> int) f, int i)
    return f(f(i))

fun int add1(int x)
    return x + 1

